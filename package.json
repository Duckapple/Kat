{
    "name": "kat.js",
    "version": "1.0.0",
    "description": "A kattis client",
    "main": "dist/index.js",
    "bin": {
        "main": "./dist/index.js"
    },
    "scripts": {
        "build": "NODE_ENV=production tsc",
        "dev": "NODE_ENV=development tsc",
        "watch": "nodemon src/index.ts",
        "start": "tsc && node dist/index.js",
        "lint": "eslint 'src/**/*.{js,ts,tsx}'",
        "test": "jest",
        "test:ci": "jest --testLocationInResults --ci --json",
        "typeorm": "tsc && npx typeorm"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jlndk/Kat.js.git"
    },
    "keywords": [
        "kattis"
    ],
    "author": "jlndk",
    "bugs": {
        "url": "https://github.com/jlndk/Kat.js/issues"
    },
    "homepage": "https://github.com/jlndk/Kat.js#readme",
    "dependencies": {
        "got": "^11.7.0",
        "pretty-error": "^2.1.1",
        "unzipper": "^0.10.11",
        "yargs": "^16.0.3"
    },
    "devDependencies": {
        "@types/nock": "^11.1.0",
        "@types/node": "^14.11.2",
        "@types/unzipper": "^0.10.3",
        "@types/yargs": "^15.0.7",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.4.2",
        "nock": "^13.0.4",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.2",
        "ts-jest": "^26.4.1",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3"
    }
}
